@model WebsiteTuyenSinh.Models.PhongKhoaViewModel
@{
    ViewBag.Title = "Chi tiết phòng khoa";
    Layout = "~/Views/Shared/_RightBody.cshtml";
}
    <div class="block-nopadding no-padding-home">
        <div class="content noidung">
            <div class="detail-news">
                <div class="content-block">
                    <div class="col-sm-12 no-padding-left padding-bottom">
                        <span class="title-block text-blue nopadding">@Model.TenPhongKhoa</span>
                    </div>
                    <div class="col-sm-12 nopadding news-info"></div>
                    <panel id="contentnews">
                        @Html.Raw(Model.NoiDung)
                    </panel>
                    <div class="addthis_inline_share_toolbox pull-right"></div>
                </div>
            </div>
        </div>
    </div>
